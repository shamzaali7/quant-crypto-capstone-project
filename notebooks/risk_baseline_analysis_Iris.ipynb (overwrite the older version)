      "source": [
        "# Export HAR predictions (walk-forward expanding window, no lookahead)\n",
        "import numpy as np\n",
        "import os\n",
        "import pandas as pd\n",
        "from sklearn.linear_model import LinearRegression\n",
        "\n",
        "X_COLS = ['rv_1', 'rv_5', 'rv_22']\n",
        "MIN_TRAIN = 365  # minimum history before first forecast (can set to 252 if preferred)\n",
        "\n",
        "har_oos = har.copy()\n",
        "har_oos['vol_pred_har_oos'] = np.nan\n",
        "\n",
        "for i in range(MIN_TRAIN, len(har_oos)):\n",
        "    train_slice = har_oos.iloc[:i]\n",
        "    X_train = train_slice[X_COLS]\n",
        "    y_train = train_slice['y_vol']\n",
        "\n",
        "    model = LinearRegression()\n",
        "    model.fit(X_train, y_train)\n",
        "\n",
        "    x_i = har_oos[X_COLS].iloc[[i]]\n",
        "    har_oos.iloc[i, har_oos.columns.get_loc('vol_pred_har_oos')] = float(model.predict(x_i)[0])\n",
        "\n",
        "# Enforce non-negativity\n",
        "har_oos['vol_pred_har_oos'] = har_oos['vol_pred_har_oos'].clip(lower=0)\n",
        "\n",
        "pred_har = (\n",
        "    har_oos.dropna(subset=['vol_pred_har_oos'])[['vol_pred_har_oos']]\n",
        "    .rename(columns={'vol_pred_har_oos': 'vol_pred'})\n",
        "    .reset_index()\n",
        "    .rename(columns={har_oos.index.name or 'index': 'date'})\n",
        ")\n",
        "\n",
        "out_path_har = os.path.join(OUT_DIR, \"risk_baseline_har_vol_pred_v0_2.csv\")\n",
        "os.makedirs(os.path.dirname(out_path_har), exist_ok=True)\n",
        "pred_har.to_csv(out_path_har, index=False)\n",
        "\n",
        "print(\"Saved:\", out_path_har, \"| shape:\", pred_har.shape)\n",
        "display(pred_har.head())\n",
        "display(pred_har.tail())\n"
      ]
