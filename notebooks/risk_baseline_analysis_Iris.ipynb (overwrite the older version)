        "import numpy as np\n",
        "import pandas as pd\n",
        "\n",
        "# df should already exist in your notebook with your BTC price series loaded\n",
        "# PRICE_COL should already be set to the BTC close/last price column\n",
        "\n",
        "df_eval = df.copy()\n",
        "\n",
        "# Fix: If DATE_COL is the index, reset it so it becomes a column\n",
        "if DATE_COL not in df_eval.columns and df_eval.index.name == DATE_COL:\n",
        "    df_eval = df_eval.reset_index()\n",
        "\n",
        "df_eval[DATE_COL] = pd.to_datetime(df_eval[DATE_COL])\n",
        "df_eval = df_eval.sort_values(DATE_COL).set_index(DATE_COL)\n",
        "\n",
        "price = pd.to_numeric(df_eval[PRICE_COL], errors=\"coerce\")\n",
        "ret = np.log(price).diff()\n",
        "\n",
        "# Realized volatility proxy for the day (actual)\n",
        "df_eval[\"y_vol\"] = ret.abs()"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 17,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "qUmhJelQ7-2f",
        "outputId": "50aa1ed8-81f8-4769-b64d-fb3b3d7b3415"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Risk Baseline v0.1 (last 365 days)\n",
            "MAE: 0.012938710653363663\n",
            "RMSE: 0.016152262038407333\n",
            "log-RMSE: 1.5180283501068088\n"
          ]
        }
      ],
      "source": [
        "pred = pd.read_csv(out_path)\n",
        "pred[\"date\"] = pd.to_datetime(pred[\"date\"])\n",
        "pred = pred.set_index(\"date\")\n",
        "\n",
        "merged = df_eval[[\"y_vol\"]].join(pred[[\"vol_pred\"]], how=\"inner\").dropna()\n",
        "\n",
        "# Holdout: last 365 days\n",
        "test_size = 365\n",
        "test = merged.iloc[-test_size:]\n",
        "\n",
        "mae  = float(np.mean(np.abs(test[\"y_vol\"] - test[\"vol_pred\"])))\n",
        "rmse = float(np.sqrt(np.mean((test[\"y_vol\"] - test[\"vol_pred\"])**2)))\n",
        "\n",
        "eps = 1e-12\n",
        "log_rmse = float(np.sqrt(np.mean((np.log(test[\"y_vol\"]+eps) - np.log(test[\"vol_pred\"]+eps))**2)))\n",
        "\n",
        "print(\"Risk Baseline v0.1 (last 365 days)\")\n",
        "print(\"MAE:\", mae)\n",
        "print(\"RMSE:\", rmse)\n",
        "print(\"log-RMSE:\", log_rmse)\n"
      ]
    },
